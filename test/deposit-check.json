        {"post": {
          "url": "/buyFund",
          "json": {"symbol": "{{username}}-FUND", "cashValue": "1"},
          "match":{
            "json":"$.message",
            "value":"The fund has been successfully purchased"
          }
        }},
        {"get": {
          "url": "/viewPortfolio",
          "afterResponse": "parsePortfolio",
          "match":[
            {
              "json":"$.message",
              "value":"The action was successful"
            },
            {
              "json":"$.cash",
              "value":"6.86"
            },
            {
              "json":"$.$shares['{{username}}-FUND']",
